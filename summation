5:-
#include<stdio.h>
int compare(int a,int b);
void main()
{
    Int a,b,large;
    printf("enter two numbers:");
    scanf("%d%d",&a,&b);
    
    large=compare(a,b);
    printf("largest=%d,large);
    
}

int compare(int a,int b)
{
    if(a>b)
    return a;
    else
    return b;
}

6:-
#include<stdio.h>
int compare(int a,int b,int c);
void main()
{
    int a,b,c,large;
    printf("enter three numbers:");
    scanf("%d%d%d",&a,&b,&c);
    
    large=compare(a,b,c);
    printf("largest=%d",large);
    
}

int compare(int a,int b,int c)
{
    if(a>b && a>c)
    return a;
    if(b>a && b>c)
    return b;
    else
    return c;
}
