1:-
#include<stdio.h>
void main()
{
    int x,y;
    printf("enter the value of x and y:");
    scanf("%d%d"&x,&y);
    printf("the sum of %d and %d is:%d",x,y,(x+y));
}



2:-#include<stdio.h>
int input()
{
    int a;
    printf("\n enter the number:");
    scanf("%d",&a);
    return a;
}
void display(int sum)
 {   
    printf("sum=%d",sum);
 }

int add(int a,int b)
{
    int sum;
    sum=a+b;
    return sum;
}
void main()
{
    int a,b,sum=0;
 a= input();
  b= input();
  sum =add(a,b);
  display(sum);
}






